{
  "name": "@fieldhive/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "ws:dev": "ts-node-dev --respawn --transpile-only src/websockets/server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:ws": "node dist/websockets/server.js",
    "check-db": "ts-node src/scripts/check-db.ts",
    "check-tables": "ts-node src/scripts/check-tables.ts",
    "check-properties": "ts-node src/scripts/check-properties.ts",
    "generate-migration": "ts-node src/scripts/generate-migration.ts",
    "run-migrations": "ts-node src/scripts/run-migrations.ts",
    "revert-migration": "ts-node src/scripts/revert-migration.ts",
    "setup-equipment": "ts-node src/scripts/setup-equipment.ts",
    "drop-tables": "ts-node src/scripts/drop-tables.ts",
    "migrate-account-settings": "ts-node src/scripts/migrate-account-settings.ts",
    "cleanup-account-settings": "ts-node src/scripts/cleanup-account-settings.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fieldhive/infrastructure": "workspace:*",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/geojson": "^7946.0.14",
    "@types/node": "^20.4.5",
    "@types/ws": "^8.5.13",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "geojson": "^0.5.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.1.6",
    "winston": "^3.10.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}